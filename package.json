{
  "name": "jamf-inspector",
  "version": "0.1.0",
  "description": "Jamf Pro ITIL/ITAM Dashboard - Strictly typed, functional, and documented.",
  "main": "backend/index.mts",
  "type": "module",
  "frontend": "frontend/src/main.tsx",
  "scripts": {
    "start": "ts-node backend/index.ts",
    "build:frontend": "cd frontend && vite build",
    "build:backend": "npx tsc --project tsconfig.json",
    "serve:frontend:dev": "cd frontend && vite --host",
    "serve:backend:dev": "tsx backend/index.mts",
    "serve:all": "npm run build:frontend && npm run build:backend && node dist/backend/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "serve:all:dev": "npm-run-all --parallel serve:frontend:dev serve:backend:dev",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write ."
  },
  "vitest": {
    "setupFiles": "./vitest.setup.ts",
    "environment": "jsdom"
  },
  "author": "Jeremiah Pegues",
  "license": "SEE LICENSE IN docs/AUTHORITATIVE.md",
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/axios": "^0.9.36",
    "@types/node-cache": "^4.1.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.11.0",
    "express": "^4.19.2",
    "framer-motion": "^12.23.11",
    "node-cache": "^5.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "vite": "^5.4.19"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/react-redux": "^7.1.34",
    "@types/redux": "^3.6.31",
    "@types/testing-library__react": "^10.0.1",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
