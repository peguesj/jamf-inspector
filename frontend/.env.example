# Jamf Inspector Frontend Environment Example

# LLM Providers
LLM_PROVIDER=OpenAI
OPENAI_API_KEY=
OPENAI_API_URL=https://api.openai.com/v1
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_API_URL=
GEMINI_API_KEY=
GEMINI_API_URL=
OLLAMA_API_URL=http://localhost:11434
DEEPSEEK_API_KEY=
DEEPSEEK_API_URL=
QWEN_API_KEY=
QWEN_API_URL=

# LLM Advanced Options
LLM_MODEL=gpt-4o
LLM_LOG_LEVEL=info
LLM_LOG_LOCATION=logs/llm.log
LLM_RATE_LIMIT=60
LLM_TOKEN_LIMIT=4096
LLM_TIMEOUT_MS=30000

# Dashboard/Frontend
DASHBOARD_THEME=liquid-glass
DASHBOARD_PRESET=default
DASHBOARD_WEBHOOK_URL=

# Notifications
NOTIFICATION_ENABLED=true
NOTIFICATION_WEBHOOK_URL=

# Other Useful Variables
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_LOG_LEVEL=info
REACT_APP_LOG_LOCATION=logs/frontend.log
REACT_APP_FEATURE_FLAGS=chat,notifications,templates

# Add any additional variables as needed
